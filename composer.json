{
    "name": "atanvarno/dependency",
    "description": "PSR-11 dependency injection container",
    "keywords": [
         "PSR-11", "container", "dependency injection", "dic", "ioc"
    ],
    "homepage": "https://github.com/atanvarno69/dependency",
    "license": "MIT",
    "authors": [
        {
            "name": "atanvarno69",
            "email": "atanvarno69@gmail.com",
            "homepage": "https://github.com/atanvarno69",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0.0",
        "psr/simple-cache": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.4",
        "atanvarno/cache-apcu": "^0.1.1"
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Atanvarno\\Dependency\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atanvarno\\Dependency\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
